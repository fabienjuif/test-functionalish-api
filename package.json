{
  "name": "@tt/workspace",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "run-p build:*",
    "api-tweets": "yarn workspace @tt/api-tweets",
    "lib-cache": "yarn workspace @tt/lib-cache",
    "lib-controllers": "yarn workspace @tt/lib-controllers",
    "lib-services": "yarn workspace @tt/lib-services",
    "lib-logger": "yarn workspace @tt/lib-logger",
    "lib-postgres": "yarn workspace @tt/lib-postgres",
    "service-tweets": "yarn workspace @tt/service-tweets",
    "build:api-tweets": "yarn api-tweets build",
    "build:lib-cache": "yarn lib-cache build",
    "build:lib-controllers": "yarn lib-controllers build",
    "build:lib-services": "yarn lib-services build",
    "build:lib-logger": "yarn lib-logger build",
    "build:lib-postgres": "yarn lib-postgres build",
    "build:service-tweets": "yarn service-tweets build",
    "ci:prettier": "prettier --check .",
    "ci": "run-p ci:*"
  },
  "workspaces": [
    "libs/*",
    "api/*",
    "services/*"
  ],
  "devDependencies": {
    "@types/node": "^17.0.24",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3"
  }
}
